{
  "version": 2,
  "builds": [{ "src": "index.html", "use": "@vercel/static-build" }],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://dikagustiana.com https://*.wordpress.com https://*.wp.com;"
        }
      ]
    }
  ]
}
