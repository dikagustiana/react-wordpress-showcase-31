{
  "version": 2,
  "builds": [{ "src": "index.html", "use": "@vercel/static-build" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestorsframe-ancestors https://dikagustiana.com https://*.wordpress.com https://*.wp.com;//*.wp.com;"
        }
      ]
    }
  ]
}
